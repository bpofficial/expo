    "i18n-js": "^3.0.11",
    "immutable": "^3.8.1",
    "jasmine-core": "^2.4.1",
    "lodash": "^4.17.4",
    "react": "16.5.0",
    "react-native": "0.57.1",
    "react-native-web": "^0.10.0",
    "sinon": "^7.1.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.2.5",
