{
  "name": "test-suite",
  "version": "1.0.0",
  "description": "Test suite for the Expo SDK",
  "main": "__generated__/AppEntry.js",
  "scripts": {
    "postinstall": "expo-yarn-workspaces postinstall",
    "test": "cd runner && yarn && cd .. && node runner/index.js .",
    "test:web": "node puppeteer-test/index.js",
    "test:web:ci": "cd puppeteer-test && yarn && cd .. && node puppeteer-test/index.js .",
    "web:dev": "webpack-dev-server -d --open --config webpack/webpack.dev.js",
    "web:build": "webpack -p --config webpack/webpack.prod.js",
    "web:build:stats": "webpack -p --config webpack/webpack.prod.js --json > web-stats.json",
    "web:publish": "netlify deploy --dir web-build",
    "web:serve": "serve ./web-build"
  },
  "author": "",
  "license": "MIT",
  "powertools": {
    "group": "client"
  },
  "dependencies": {
